{
    "name": "jenky/jsonplaceholder",
    "description": "Example of Atlas using JSON Placeholder",
    "keywords": [
        "atlas",
        "jsonplaceholder",
        "sdk"
    ],
    "homepage": "https://github.com/jenky/jsonplaceholder",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "cuyz/valinor": "^1.4",
        "guzzlehttp/guzzle": "^7.7",
        "jenky/atlas": "^0.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.10",
        "jenky/atlas-mock-client": "^1.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\JsonPlaceholder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\JsonPlaceholder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse"
        ],
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "test": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage"
        ],
        "check": [
            "@cs --dry-run",
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
